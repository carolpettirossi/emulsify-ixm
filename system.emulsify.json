{
  "name": "emulsify-ixm",
  "description": "Design system for IXM using Emulsify",
  "homepage": "https://github.com/carolpettirossi/emulsify-ixm.git",
  "repository": "https://github.com/carolpettirossi/emulsify-ixm.git",
  "structure": [
    {
      "name": "components",
      "description": "Components commonly used in higher-level components"
    }
  ],
  "variants": [
    {
      "platform": "drupal",
      "structureImplementations": [
        {
          "name": "components",
          "directory": "./src/components/"
        }
      ],
      "components": [
        {
          "name": "button",
          "structure": "components",
          "required": true,
          "dependency": [
            "icons"
          ]
        },
        {
          "name": "icons",
          "structure": "components",
          "required": true,
          "dependency": []
        }
      ]
    }
  ]
}